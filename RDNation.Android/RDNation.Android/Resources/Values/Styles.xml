<resources>
	<style name="Theme.Splash" parent="android:Theme">
		<item name="android:background">@drawable/splash</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:colorBackground">@color/white</item>
		<item name="android:scaleType">center</item>
	</style>
	<style name="spinnerItemStyle" parent="android:Widget.TextView.SpinnerItem">
		<item name="android:textColor">@color/purple</item>
	</style>

	<style name="spinnerDropDownItemStyle" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
		<item name="android:textColor">@color/purple</item>
	</style>
	<style name="mainButtons" >
		<item name="android:layout_marginBottom">5dp</item>
		<item name="android:layout_marginLeft">5dp</item>
		<item name="android:layout_marginRight">5dp</item>
		<item name="android:gravity">left|center_vertical</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:drawableRight">@drawable/ic_action_right</item>
	</style>
	<style name="profileName" >
		<item name="android:gravity">left</item>
		<item name="android:paddingTop">7dp</item>
		<item name="android:paddingBottom">7dp</item>
		<item name="android:paddingLeft">7dp</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:textSize">16dp</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="profileTableStat" >
		<item name="android:gravity">center_horizontal</item>
		<item name="android:textSize">14dp</item>
	</style>
	<style name="contentBoxHead">
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_marginTop">4dp</item>
	</style>
	<style name="contentBoxText">
		<item name="android:textColor">@color/black</item>
	</style>
	<style name="teamNameBtn">
		<item name="android:gravity">left|center_vertical</item>
		<item name="android:textSize">13dp</item>
		<item name="android:padding">4dp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:drawablePadding">5dp</item>
	</style>
	<style name="actionbarprogressbar" parent="@android:style/Widget.ProgressBar.Small.Inverse">
	</style>
	<style name="profileInfoTableHead">
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@color/purple</item>
		<item name="android:padding">7dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="profileInfoTableContent">
		<item name="android:textColor">@color/purple</item>
		<item name="android:padding">7dp</item>
	</style>
	<style name="leagueInfoTableHead">
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:padding">7dp</item>
		<item name="android:gravity">center_horizontal</item>
		<item name="android:background">@color/purple</item>
		<item name="android:singleLine">true</item>
		<item name="android:ellipsize">end</item>

	</style>
	<style name="publicLeagueEventRowStyle">
		<item name="android:textColor">@color/black</item>
		<item name="android:paddingTop">7dp</item>
		<item name="android:paddingBottom">7dp</item>
		<item name="android:gravity">center_horizontal</item>
		<item name="android:singleLine">true</item>
		<item name="android:ellipsize">end</item>

	</style>
	<style name="publicLeagueEventRowStyleName">
		<item name="android:textColor">@color/black</item>
		<item name="android:paddingTop">7dp</item>
		<item name="android:paddingBottom">7dp</item>


	</style>

	<style name="publicLeagueMemberRowStyle">
		<item name="android:textColor">@color/black</item>
		<item name="android:paddingTop">7dp</item>
		<item name="android:paddingBottom">7dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="gamesRow">
		<item name="android:layout_marginTop">4dp</item>
		<item name="android:layout_marginBottom">4dp</item>
	</style>
	<style name="publicGameRowHeader">
		<item name="android:paddingLeft">10dp</item>
		<item name="android:background">@color/purple</item>
		<item name="android:textColor">@color/white</item>
	</style>
	<style name="publicGameRow">
		<item name="android:padding">5dp</item>
		<item name="android:background">@color/black</item>
	</style>
	<style name="publicGameRowTeamName">


	</style>
	<style name="publicEventRowStyle">
		<item name="android:textColor">@color/black</item>
		<item name="android:paddingTop">7dp</item>
		<item name="android:paddingBottom">7dp</item>
		<item name="android:gravity">left</item>

	</style>
	<style name="publicEventRowTitleStyle">
		<item name="android:textColor">@color/purple</item>
		<item name="android:paddingTop">7dp</item>
		<item name="android:paddingBottom">7dp</item>
		<item name="android:gravity">left</item>
		<item name="android:textSize">18dp</item>
	</style>
	<style name="publicEventRow">
		<item name="android:background">@color/white</item>
		<item name="android:layout_marginTop">4dp</item>
		<item name="android:layout_marginBottom">4dp</item>
		<item name="android:textSize">18dp</item>
	</style>
	<style name="publicEventProfileeventName">
		<item name="android:textColor">@color/purple</item>
		<item name="android:layout_marginTop">2dp</item>
		<item name="android:layout_marginBottom">4dp</item>
		<item name="android:textSize">20dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="publicEventProfileeventLocation">
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_marginTop">2dp</item>
		<item name="android:layout_marginBottom">2dp</item>
		<item name="android:textSize">15dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="publicEventProfileeventAddress">
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_marginTop">2dp</item>
		<item name="android:layout_marginBottom">2dp</item>
		<item name="android:textSize">15dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="publicEventProfileeventDateTime">
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_marginTop">2dp</item>
		<item name="android:layout_marginBottom">2dp</item>
		<item name="android:textSize">15dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="publicEventProfileeventOrganizerInfo">
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_marginTop">2dp</item>
		<item name="android:layout_marginBottom">2dp</item>
		<item name="android:textSize">15dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="publicEventProfileeventDescription">
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_margin">4dp</item>
		<item name="android:textSize">12dp</item>
		<item name="android:gravity">center_horizontal</item>
	</style>
	<style name="publicShopRowTitleStyle">
		<item name="android:textColor">@color/purple</item>
		<item name="android:paddingTop">7dp</item>
		<item name="android:paddingBottom">2dp</item>
		<item name="android:gravity">left</item>
		<item name="android:textSize">18dp</item>
	</style>
	<style name="publicShopRowStyle">
		<item name="android:textColor">@color/black</item>
		<item name="android:gravity">left</item>
	</style>
	<style name="publicShopRowPriceStyle">
		<item name="android:textColor">@color/black</item>
		<item name="android:gravity">left</item>
	</style>
	<style name="infoSection">
		<item name="android:textColor">@color/purple</item>
		<item name="android:layout_marginTop">2dp</item>
		<item name="android:layout_marginBottom">2dp</item>
		<item name="android:padding">2dp</item>
	</style>
	<style name="buyNowButton">
		<item name="android:textColor">@color/white</item>
	</style>
	<style name="publicSkaterRowName">
		<item name="android:textSize">20dip</item>
		<item name="android:textColor">#FF7F3300</item>
	</style>
	<style name="publicSkaterRowOther">
		<item name="android:textSize">15dip</item>
		<item name="android:textColor">@color/black</item>
	</style>
</resources>