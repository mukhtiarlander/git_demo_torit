@{Guid memId = RDN.Library.Classes.Account.User.GetMemberId();
  bool isPaid = RDN.Library.Cache.MemberCache.CheckIsLeagueSubscriptionPaid(memId);
  bool isManager = RDN.Library.Cache.MemberCache.IsManagerOrBetterOfLeague(memId);
  bool isSec = RDN.Library.Cache.MemberCache.IsSecretaryOrBetterOfLeague(memId);
  bool isMed = RDN.Library.Cache.MemberCache.IsMedicalOrBetterOfLeague(memId);
}

<div id="membersTypeButtons" class="btn-group" role="group">
    <a tag="roster" class="btn btn-default" href="@Url.Content("~/league/members/view/all")">Roster</a>
    @if (isSec)
    {
        <a tag="dates" class="btn btn-default" href="@Url.Content("~/league/members/view/dates")">Dates</a>
    }
    <a tag="dayjobs" class="btn btn-default" href="@Url.Content("~/league/members/view/jobs")">Day Jobs</a>
    @if (isPaid)
    {
        <a tag="groups" class="btn btn-default" href="@Url.Content("~/league/groups")">Groups</a>

            <a tag="insurancenumbers" class="btn btn-default" href="@Url.Content("~/league/members/view/insurance")">Insurance Numbers</a>

        if (isMed || isSec)
        {
            <a tag="medical" class="btn btn-default" href="@Url.Content("~/league/members/view/medical")">Medical</a>
        }
    }
    else
    {
        <a tag="groups" class="btn btn-default" href="@Url.Content("~/billing/league/" + ViewBag.LeagueId + "?u=" + RDN.Library.Util.Enum.SiteMessagesEnum.pp)" title="Groups are only available to subscribed leagues">Groups</a>

    <a tag="insurancenumbers" class="btn btn-default" href="@Url.Content("~/billing/league/" + ViewBag.LeagueId + "?u=" + RDN.Library.Util.Enum.SiteMessagesEnum.pp)" title="Insurance Numbers are only available to subscribed leagues">Insurance Numbers</a>

        if (isSec || isMed)
        {
            <a tag="medical" class="btn btn-default" href="@Url.Content("~/billing/league/" + ViewBag.LeagueId + "?u=" + RDN.Library.Util.Enum.SiteMessagesEnum.pp)" title="Medical Information is only available to subscribed leagues">Medical</a>
        }
    }
    @if (isManager)
{
    <a tag="permissions" class="btn btn-default" href="@Url.Content("~/league/members/view/permissions")">Permissions</a>
}


    <a tag="skatinglevel" class="btn btn-default" href="@Url.Content("~/league/members/view/class")">Skating Level</a>

    <a tag="map" class="btn btn-default" href="@Url.Content("~/league/members/view/map")">Map</a>

    <a tag="removed" class="btn btn-default" href="@Url.Content("~/league/members/view/removed")">Removed</a>


    @*<a class="btn btn-default" href="@Url.Content("~/league/members/view/all")">Roster</a>

    @if (isSec)
    {
        <a class="btn btn-default" href="@Url.Content("~/league/members/view/dates")">Dates</a>
    }
    <a class="btn btn-default" href="@Url.Content("~/league/members/view/jobs")">Day Jobs</a>*@

</div>
    